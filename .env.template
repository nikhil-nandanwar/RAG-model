# Example environment variables for local development and to show which secrets Render needs.
# Copy this to `.env` for local testing, but DO NOT commit real secrets.

# API key for Google Gemini / Vertex AI. On Render, set this as a secret env var (GEMINI_API_KEY).
GEMINI_API_KEY=

# Alternate name sometimes used
GOOGLE_API_KEY=

# If you want the app to fail fast when Gemini key is missing, set to true in production
REQUIRE_GEMINI=false

# The port the app binds to (Render sets this automatically to $PORT)
PORT=5000
